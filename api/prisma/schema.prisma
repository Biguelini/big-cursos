generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model courses {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  content     String[]
  description String
  image       String
  instructor  String
  name        String   @unique
  subtitle    String
  videos      String[]
}

model user {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  email    String @unique
  name     String
  password String
}
